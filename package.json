{
  "name": "js-signalr",
  "version": "0.0.1",
  "description": "A fork from SignalR Client for advanced propose",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "directories": {
    "test": "spec"
  },
  "scripts": {
    "clean": "node ./node_modules/rimraf/bin.js ./dist",
    "prebuild": "yarn clean",
    "build": "yarn tsc",
    "prepare": "yarn build",
    "lint": "eslint --ext '.ts' ./src && eslint --ext '.ts' ./tests",
    "prettierAll": "prettier --write ./src && prettier --write ./tests",
    "test": "jest"
  },
  "keywords": [
    "signalr",
    "js-signalr",
    "hosseinmd"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hosseinmd/js-signalr.git"
  },
  "author": "Microsoft",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hosseinmd/js-signalr/issues"
  },
  "homepage": "https://github.com/hosseinmd/js-signalr#readme",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "devDependencies": {
    "@types/eventsource": "^1.0.2",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "@types/tough-cookie": "^2.3.6",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "commitlint": "^11.0.0",
    "eslint": "^7.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "es6-promise": "^4.2.2",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "prettier": "^2.2.0",
    "prettier-plugin-jsdoc": "^0.2.5",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "eventsource": "^1.0.7",
    "node-fetch": "^2.6.0",
    "abort-controller": "^3.0.0",
    "fetch-cookie": "^0.7.3",
    "ws": "^6.0.0"
  }
}